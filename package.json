{
  "name": "pimcore-skeleton",
  "version": "5.6",
  "description": "A full-featured Pimcore skeleton for developing projects with Docker and Webpack",
  "author": "w-vision AG <support@w-vision.ch>",
  "license": "GPL-3.0",
  "homepage": "https://www.w-vision.ch",
  "repository": "https://github.com/w-vision/pimcore-skeleton",
  "scripts": {
    "build": "./node_modules/.bin/encore production",
    "deploy": "php vendor/bin/dep deploy",
    "deploy:dev": "php vendor/bin/dep deploy dev",
    "deploy:prod": "php vendor/bin/dep deploy production",
    "dev": "./node_modules/.bin/encore dev",
    "dev:server": "./node_modules/.bin/encore dev-server --hot",
    "icons:custom": "cd node_modules/uikit && yarn && yarn compile && yarn icons -- --custom ../../assets/custom/icons",
    "ssh": "php vendor/bin/dep ssh",
    "watch": "./node_modules/.bin/encore dev --watch"
  },
  "devDependencies": {
    "@symfony/webpack-encore": "^0.27.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^2.0.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.10.1",
    "stylelint-webpack-plugin": "^0.10.5"
  },
  "dependencies": {
    "@uppy/core": "^0.30.5",
    "@uppy/dashboard": "^0.30.5",
    "@uppy/xhr-upload": "^0.30.5",
    "axios": "^0.18.0",
    "core-js": "^3.0.1",
    "foundation-emails": "^2.2.1",
    "imagesloaded": "^4.1.4",
    "mapbox-gl": "^0.53.1",
    "mapbox-gl-geocoder": "^2.0.1",
    "prettier-bytes": "^1.0.4",
    "uikit": "^3.1.7",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": [
    ">0.25%",
    "not ie <= 10",
    "not op_mini all"
  ]
}
